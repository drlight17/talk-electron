{
  "name": "talk-electron",
  "productName": "NC Talk Electron",
  "description": "Talk web embedded app",
  "keywords": [
    "nextcloud",
    "talk",
    "spreed",
    "electron",
    "win7"
  ],
  "icon": "icon.png",
  "main": "./main.js",
  "version": "0.2.6-alpha",
  "author": "drlight17",
  "build": {
    "appId": "com.electron.talk-electron",
    "productName": "NC Talk Electron",
    "asar": true,
    "asarUnpack": [
      "**/node_modules/sharp/**/*",
      "**/node_modules/@img/**/*"
    ],
    "nsis": {
      "oneClick": true,
      "perMachine": true,
      "allowElevation": true
    },
    "msi": {
      "oneClick": true,
      "perMachine": true
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "arm64"
          ]
        },
        {
          "target": "dmg",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "win": {
      "icon": "icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "msi",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  },
  "scripts": {
    "dist": "electron-builder",
    "start": "electron ."
  },
  "dependencies": {
    "@electron/packager": "latest",
    "@paymoapp/electron-shutdown-handler": "latest",
    "electron-fetch": "^1.9.1",
    "electron-prompt": "latest",
    "electron-store": "^8.2.0",
    "sharp": "^0.32"
  },
  "devDependencies": {
    "@paymoapp/electron-shutdown-handler": "latest",
    "electron": "^22.3.27",
    "electron-builder": "^24.13.3"
  }
}
